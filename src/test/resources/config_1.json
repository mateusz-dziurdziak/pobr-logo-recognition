{
  "outputDir": "outDir",
  "outputFileName": "outFile",
  "exportFileAfterEachStep": true,
  "filters": [
    {
      "type": "Min",
      "windowSize": 9
    },
    {
      "type": "Max",
      "windowSize": 9
    },
    {
      "type": "Rank",
      "windowSize": 9,
      "position": 5
    },
    {
      "type": "Binary"
    },
    {
      "type": "Mask",
      "mask": [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
    }
  ],
  "segmentationConfig": {
    "minPixelsForSegment": 30
  }
}
